	//Properties
	bError 			: 	BOOL := FALSE;
	fNominalVolFlow	: 	REAL := 8820;
	fVolFlowSet 	: 	REAL := 0;
	fVolFlowAct		: 	REAL;
	
	bFinish 		:	BOOL;
	
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
